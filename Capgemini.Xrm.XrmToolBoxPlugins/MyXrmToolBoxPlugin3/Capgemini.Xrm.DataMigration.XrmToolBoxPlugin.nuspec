    <tags>XrmToolBox Plugins</tags>
    <dependencies>
      <group>
        <dependency id="XrmToolBox" version="1.2020.6.40" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="bin\Release\*" target="lib\net462\Plugins"></file>
  </files>
</package>